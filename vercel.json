{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "edge"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/index.html" }
  ]
}
